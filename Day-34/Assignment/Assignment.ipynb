   "source": [
    "1. Prompt the user to enter a number. \n",
    "2. Print the multiplication table of the entered number from 1 to 10 using a for loop."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Multiplication Table of 4 is:\n",
      "4*1=4\n",
      "4*2=8\n",
      "4*3=12\n",
      "4*4=16\n",
      "4*5=20\n",
      "4*6=24\n",
      "4*7=28\n",
      "4*8=32\n",
      "4*9=36\n",
      "4*10=40\n"
     ]
    }
   ],
   "source": [
    "num=int(input(\"Enter a Number: \"))\n",
    "print(f\"Multiplication Table of {num} is:\")\n",
    "for i in range(1, 11):\n",
    "    mult=num*i\n",
    "    print(f\"{num}*{i}={mult}\")"
   ]
  }
