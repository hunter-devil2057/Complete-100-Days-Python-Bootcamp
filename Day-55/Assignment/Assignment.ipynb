   "source": [
    "1. Prompt the user to enter a number. \n",
    "2. Print the multiplication table of the entered number from 1 to 10 using a loop. "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Multiplication Table of 12 is: \n",
      "12 * 1 = 12\n",
      "12 * 2 = 24\n",
      "12 * 3 = 36\n",
      "12 * 4 = 48\n",
      "12 * 5 = 60\n",
      "12 * 6 = 72\n",
      "12 * 7 = 84\n",
      "12 * 8 = 96\n",
      "12 * 9 = 108\n",
      "12 * 10 = 120\n"
     ]
    }
   ],
   "source": [
    "num=int(input(\"Enter a Number: \"))\n",
    "print(f\"Multiplication Table of {num} is: \")\n",
    "for i in range(1, 11):\n",
    "    prod=num*i\n",
    "    print(f\"{num} * {i} = {prod}\")"
   ]
  }
