      "12*10=120\n",
      "12*9=108\n",
      "12*8=96\n",
      "12*7=84\n",
      "12*6=72\n",
      "12*5=60\n",
      "12*4=48\n",
      "12*3=36\n",
      "12*2=24\n",
      "12*1=12\n"
     ]
    }
   ],
   "source": [
    "def Table(num):\n",
    "    for i in range(10, 0, -1):\n",
    "        product=num*i\n",
    "        print(f\"{num}*{i}={product}\")\n",
    "num=int(input(\"Enter a Number: \"))\n",
    "Table(num)"
   ]
  }
